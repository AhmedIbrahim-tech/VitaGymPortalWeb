@model ErrorViewModel
@{
    ViewData["Title"] = "403 - Access Forbidden";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="main-wrapper">
    <div class="error-box">
        <div class="error-img">
            <img src="~/assets/img/authentication/error-404.png" class="img-fluid" alt="Img">
        </div>
        <h3 class="h2 mb-3">Oops, something went wrong</h3>
        <p>Error 403 Access Forbidden. You don't have permission to access this resource.
            Please contact your administrator if you believe this is an error</p>
        <a href="@Url.Action("Index", "Home")" class="btn btn-primary">Back to Dashboard</a>
    </div>
</div>

